{
  "name": "asset-flow",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=10.2.4",
    "node": ">=20.11.1"
  },
  "scripts": {
    "dev": "next dev -p 4040",
    "build": "next build",
    "start": "next start -p 4040",
    "eslint": "eslint src/**/*.{ts,tsx} --fix --config .eslintrc.json",
    "stylelint": "stylelint **/*.{css,scss} --fix",
    "prettier": "prettier --log-level error --write **/*.{ts,tsx,css,scss}",
    "stop": "npx kill-port 4040"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{(t|j)s?(x),?(s)css}": [
      "npm run prettier",
      "git add ."
    ],
    "*.?(s)css": [
      "git add ."
    ],
    "*.ts?(x)": [
      "npm run eslint",
      "git add ."
    ]
  },
